    "@tamagui/font-inter": "1.74.8",
    "@tamagui/lucide-icons": "1.74.8",
    "@types/react": "~18.2.14",
    "dotenv": "^16.3.1",
    "expo": "^50.0.6",
    "expo-font": "^11.10.2",
    "expo-image": "^1.10.6",
    "expo-router": "~3.4.7",
    "expo-status-bar": "~1.11.1",
